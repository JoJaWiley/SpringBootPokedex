{{>header}}

<div class="container">
    <div class="row">
        {{#pokemon}}
        <div class="col-sm">

            <div class="card" style="width: 18rem;">
                <img class="card-img-top" src="" alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title">{{name}}</h5>
                    <p class="card-text">{{type}}</p>
                </div>
                <ul class="list-group list-group-flush">
                    <li class="list-group-item">Property1</li>
                    <li class="list-group-item">Property2</li>
                    <li class="list-group-item">Property3</li>
                </ul>
                <div class="card-body">
                    <a href="#" class="card-link">Edit</a>
                    <a href="#" class="card-link">Delete</a>
                </div>
            </div>

        </div>
        {{/pokemon}}
    </div>
</div>